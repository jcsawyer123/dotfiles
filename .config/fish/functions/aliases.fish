# Navigation
alias ..="cd .."
alias ...="cd ../.."
alias ....="cd ../../.."
alias .....="cd ../../../../"

# Shortcuts
alias fishconfig="nano ~/dotfiles/.config/fish/config.fish"
alias fishalias="nano ~/dotfiles/.config/fish/functions/aliases.fish"
alias fishreload="source ~/.config/fish/config.fish"
alias dotsync="~/dotfiles/bootstrap.sh"
alias sn="shutdown now"
alias pintel="sudo prime-select intel"
alias pnvidia="sudo prime-select nvidia"
alias pwhat="prime-select query"

